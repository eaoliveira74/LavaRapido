# Copy to .env and fill values
# Visual Crossing API key (required for Visual Crossing proxy to return data)
VISUALCROSSING_API_KEY=

# JWT secret used by the server for admin tokens
JWT_SECRET=change_this_secret

# Optional: bcrypt hash of the admin password (if not set a default demo password is used)
ADMIN_PASSWORD_HASH=

# Server port (default 4000)
PORT=4000
